package com.org.service;

import com.org.dao.IProduit;

public class SingletonService {

	private static SingletonService singletonService;
	
	public static ProdServiceImpl getService() {
		return service;
	}



	private static ProdServiceImpl service;
	private static IProduit dao;
	
	

	static{
		
		service = new ProdServiceImpl();
		service.setDao(service); 
		service.initailisation();
	}
}
